<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    {% block css %}
        {{ webpack['public/css/app.css'] }}
    {% endblock %}
    <title>
        Host Service | {% block title %}{% endblock %}
    </title>
</head>
<body>


    {% block navbar %}
        {% include 'nav.jinja' %}
    {% endblock %}

    {% block body %}{% endblock %}

    {% block js %}
        {{ webpack['public/js/app.js'] }}
        {{ webpack['public/runtime.js'] }}
    {% endblock %}
</body>
</html>